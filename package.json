{
  "name": "sisdn",
  "private": true,
  "version": "0.0.1",
  "description": "A frontend for Sisdn https://github.com/sisdn",
  "main": "",
  "scripts": {
    "clean": "rimraf dist/*",
    "copyfonts": "copyfiles -f ./src/fonts/* ./dist/fonts",
    "copyfiles": "npm run copyfonts & copyfiles -f ./src/index.html ./src/fonts ./src/favicon.ico ./dist",
    "dist": "npm run copyfiles & webpack --env=dist",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "node server.js --env=dev",
    "//": "***********",
    "///": "The duplicate key for test is override",
    "////": "***********",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sisdn/sisdn.git"
  },
  "keywords": [
    "student",
    "management",
    "system",
    "sudan",
    "arabic"
  ],
  "author": "Mohammed Hashim <mhashim dot gm at gmail dot com>",
